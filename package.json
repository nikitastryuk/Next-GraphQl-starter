    "react-dom": "17.0.2",
    "urql": "^2.0.6"
  },
  "devDependencies": {
    "@types/faker": "5.5.3",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "faker": "5.5.3",
    "husky": "7.0.4",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "prisma": "3.8.1",
    "simple-git-hooks": "2.7.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
